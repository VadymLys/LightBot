{
  "name": "luzapi",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "bundle": "esbuild src/bots/Telegram/TelegramLambdaHandler.ts --bundle --platform=node --target=node18 --outfile=dist/lambda.js",
    "dev": "tsx watch src/db/dbCloud.ts",
    "dev:bot": "tsx src/bots/Telegram/LocalBot.ts",
    "telegram": "tsx src/bots/Telegram/TelegramLambdaHandler.ts",
    "whatsapp": "tsx src/bots/WhatsAppBot.ts",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "aws:dev": "serverless offline",
    "aws:build": "serverless package",
    "aws:deploy": "serverless deploy",
    "aws:remove": "serverless remove",
    "clean": "rimraf .esbuild",
    "prepare": "husky install"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.1",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "grammy": "^1.36.3",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^2.7.0",
    "pg": "^8.16.3",
    "qrcode-terminal": "^0.12.0",
    "whatsapp-web.js": "^1.31.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.149",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.15.31",
    "@types/node-fetch": "^2.6.12",
    "@types/pg": "^8.15.4",
    "@types/qrcode-terminal": "^0.12.2",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^3.2.3",
    "@vitest/ui": "^3.2.3",
    "esbuild": "^0.25.5",
    "husky": "^9.1.7",
    "rimraf": "^5.0.5",
    "serverless": "^4.17.1",
    "serverless-offline": "^14.4.0",
    "supertest": "^7.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.3",
    "vitest-mock-extended": "^3.1.0"
  }
}
